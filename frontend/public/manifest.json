  "shortcuts": [
    {
      "name": "Search Listings",
      "short_name": "Search",
      "description": "Find your perfect vacation rental",
      "url": "/search",
      "icons": [
        {
          "src": "logo192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "My Bookings",
      "short_name": "Bookings",
      "description": "View your upcoming trips",
      "url": "/bookings",
      "icons": [
        {
          "src": "logo192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Favorites",
      "short_name": "Favorites",
